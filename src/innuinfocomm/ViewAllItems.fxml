<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml" fx:controller="innuinfocomm.ViewAllItemsController">
  <children>
    <Label layoutX="350.0" layoutY="20.0" text="View All Items">
      <effect>
        <Glow />
      </effect>
      <font>
        <Font name="System Bold" size="15.0" fx:id="x1" />
      </font>
    </Label>
    <TableView fx:id="ItemsTableView" layoutX="80.0" layoutY="97.0" prefHeight="440.0" prefWidth="641.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="269.0" text="ItemName" fx:id="itemNameTableColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="154.0" text="Group" fx:id="itemGroupTableColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="60.0" text="First Unit" fx:id="itemFUnitTableColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="70.0" text="Item Code" fx:id="itemCodeTableColumn" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="86.0" text="Current Stock" fx:id="itemCurrentStockTableColumn" />
      </columns>
    </TableView>
    <TextField fx:id="searchItemsTextBox" layoutX="80.0" layoutY="68.0" onKeyReleased="#handleKeyInSearchBox" prefWidth="270.0" promptText="Start Typing to  get specific items" />
    <Label font="$x1" layoutX="361.0" layoutY="69.0" text="Search Box" textFill="#33b6ff">
      <effect>
        <InnerShadow choke="0.015873015873015872" />
      </effect>
    </Label>
  </children>
</AnchorPane>
