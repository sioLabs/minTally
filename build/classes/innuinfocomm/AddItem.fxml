<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="640.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <Label layoutX="80.0" layoutY="70.0" text="Name:" />
    <TextField fx:id="itemNameTextBox" layoutX="139.0" layoutY="67.0" prefWidth="244.0" />
    <Label layoutX="277.0" layoutY="18.0" text="Add Item">
      <effect>
        <InnerShadow blurType="GAUSSIAN">
          <input>
            <Glow />
          </input>
        </InnerShadow>
      </effect>
      <font>
        <Font size="18.0" />
      </font>
    </Label>
    <Label layoutX="80.0" layoutY="120.0" text="Under:" />
    <ComboBox fx:id="itemGroupComboBox" layoutX="139.0" layoutY="117.0" prefWidth="145.0">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ComboBox>
    <Label layoutX="80.0" layoutY="159.0" text="Units:" />
    <ComboBox fx:id="itemUnitComboBox" layoutX="139.0" layoutY="156.0" prefWidth="145.0">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ComboBox>
    <Label layoutX="80.0" layoutY="193.0" text="VAT %:" />
    <TextField fx:id="itemVatPercTextBox" layoutX="139.0" layoutY="190.0" prefWidth="56.0" />
    <Label layoutX="10.0" layoutY="239.0" text="Opening Quantity:" />
    <TextField fx:id="itemOpeningBalTextBox" layoutX="139.0" layoutY="236.0" prefWidth="82.0" />
    <Label layoutX="88.0" layoutY="288.0" text="Rate:" />
    <TextField fx:id="itemRateTextBox" layoutX="130.0" layoutY="285.0" prefWidth="82.0" />
    <Label layoutX="235.0" layoutY="287.0" text="Per:" />
    <ComboBox fx:id="itemRatePerUnitComboBox" layoutX="277.0" layoutY="285.0" prefWidth="106.0">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ComboBox>
    <Label layoutX="46.0" layoutY="329.0" text="Total Value:" />
    <TextField fx:id="itemTotalValueTextBox" layoutX="134.0" layoutY="326.0" prefWidth="82.0" />
    <Button fx:id="saveItemButton" layoutX="134.0" layoutY="378.0" mnemonicParsing="false" onAction="#handleSaveItemButton" text="Save Item" />
  </children>
</AnchorPane>
